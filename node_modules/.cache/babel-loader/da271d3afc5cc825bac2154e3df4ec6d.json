Provider v-slot=\"{ errors, classes }\" rules=\"required\" name=\"companyname\" >\n\n          <div class=\"form-group\">\n            <label class=\"text-left d-flex\">Company name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"companyname\">\n            <span :class=\"classes\" class=\"styleSpan\">{{ errors[0] }}</span>\n\n          </div>\n        </ValidationProvider>\n        <ValidationProvider v-slot=\"{ errors, classes }\" rules=\"required|numeric\" name=\"numberofemployee\" >\n\n          <div class=\"form-group \">\n            <label class=\"text-left d-flex\">number of employee</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"numberofemployee\">\n            <span :class=\"classes\" class=\"styleSpan\">{{ errors[0] }}</span>\n\n          </div>\n        </ValidationProvider>\n        <button class=\"btn btn-info\" type=\"button\" v-if=\"currentPage < totalPage \" @click=\"prevProcess\">PREVIOUS</button>\n        <button class=\"btn btn-primary ml-2\" type=\"submit\" v-if=\"currentPage < totalPage\">NEXT</button>\n        <button class=\"btn btn-danger ml-2\" type=\"button\" @click=\"reset\" v-if=\"currentPage === totalPage\">RESET</button>\n      </form>\n    </ValidationObserver>\n      <ValidationObserver ref=\"observer\" v-slot=\"{handleSubmit}\">\n        <form v-if=\"currentPage === 3\" @submit.prevent=\"handleSubmit(finishProcess)\">\n          <ValidationProvider v-slot=\"{ errors, classes }\" rules=\"required\" name=\"select\" >\n\n            <div class=\"input-group mb-3\">\n              <label>Form Where did you hear about us</label>\n              <select class=\"custom-select\" id=\"inputGroupSelect01\" v-model=\"select\">\n                <option selected>Choose...</option>\n                <option value=\"friend\">Friend</option>\n                <option value=\"facebook\">Facebook</option>\n                <option value=\"google\">Google</option>\n              </select>\n           